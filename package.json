{
  "name": "code-location",
  "main": "./lib/code-location",
  "version": "4.0.0",
  "description": "code-location is a tool that lets you know where you are in your code, by showing you your location (class, function, etc.) in the status bar of the Atom editor.",
  "keywords": [
    "location",
    "tree",
    "code",
    "editor",
    "statusbar",
    "status-bar",
    "indentation",
    "incode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonny-exe/atom-code-location.git"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
